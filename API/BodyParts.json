    "id": "BPT-001",
    "medicalName": "Ear",
    "imageName": "ear_image",
    "renderName": "ear_render",
    "procedures": [
        {
            "surgeryTechnicalName": "Tympanoplasty",
            "description": "This surgery aims to repair a hole in the eardrum and, if necessary, restore the small bones of the middle ear. It's commonly performed to improve hearing and prevent water from entering the middle ear.",
            "steps": [
                {
                    "description": "Make an incision behind the ear to access the eardrum. This provides a clear view and safe entry point for the surgery.",
                    "imageName": "",
                    "order": 1
                },
                {
                    "description": "Remove any damaged or infected tissue from the middle ear. This step is crucial for preventing further infection and ensuring a clean environment for the eardrum's reconstruction.",
                    "imageName": "",
                    "order": 2
                },
                {
                    "description": "Place a graft over the hole in the eardrum. The graft acts as a scaffold that allows the body's natural healing processes to regenerate the eardrum tissue.",
                    "imageName": "",
                    "order": 3
                },
                {
                    "description": "Close the initial incision with stitches. This step finalizes the surgery, allowing the ear to heal properly with the new graft in place.",
                    "imageName": "",
                    "order": 4
                }
            ]
        }
    ]
  },
